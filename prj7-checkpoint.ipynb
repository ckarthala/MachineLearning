    "import math\n",
    "def pac_learn_least_sample_size(n, epsilon=0.2, delta=0.1, seed=None) :\n",
    "    least_size=1/epsilon*(math.log(n)+math.log(1/delta))\n",
    "    return least_size\n",
    "least_size=pac_learn_least_sample_size(6600000)\n",
    "print(least_size)"
   ]
